{
  "name": "Mistura Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#2B2D3B",
    "editor.foreground": "#CCCCCC",
    "editor.selectionBackground": "#264f789f",
    "editor.lineHighlightBackground": "#2D3241",
    "editorCursor.foreground": "#CCCCCC",
    "editorWhitespace.foreground": "#555555",
    "editorIndentGuide.background1": "#6a6f81",
    "editorIndentGuide.activeBackground1": "#6a6f81",
    "editorError.foreground": "#FF0000",
    "editorWarning.foreground": "#76c991",
    "editorInfo.foreground": "#4F4F4F",
    "editorRuler.foreground": "#6a6f81",
    "editorGutter.background": "#2B2D3B",
    "editorGutter.modifiedBackground": "#CCCCCC",
    "editorGutter.addedBackground": "#CCCCCC",
    "editorGutter.deletedBackground": "#FF0000",
    "editor.selectionHighlightBackground": "#264f789f",
    "editor.findMatchBackground": "#8D7682",
    "editor.findMatchHighlightBackground": "#8D768299",
    "editorWidget.background": "#2D3241",
    "editorWidget.border": "#6a6f81",
    "editorSuggestWidget.background": "#2E2E2E",
    "editorSuggestWidget.border": "#6a6f81",
    "editorSuggestWidget.foreground": "#CCCCCC",
    "editorSuggestWidget.selectedBackground": "#265AA9",
    "editorSuggestWidget.highlightForeground": "#ff6d7c",
    "editorHoverWidget.background": "#2c2e3c",
    "editorHoverWidget.border": "#6a6f81",
    "editorLineNumber.foreground": "#696080",
    "editorLineNumber.activeForeground": "#D7D1E6",
    "editorGroup.border": "#6a6f81",
    "editorGroupHeader.tabsBackground": "#24252E",
    "sideBar.border": "#6a6f81",
    "activityBar.border": "#6a6f81",
    "activityBar.background": "#24252E",
    "activityBar.foreground": "#CCCCCC",
    "activityBar.activeBackground": "#2D3241",
    "activityBar.activeBorder": "#4E77FF",
    "activityBar.inactiveForeground": "#6C7390",
    "sideBar.background": "#24252E",
    "sideBar.foreground": "#CCCCCC",
    "sideBarSectionHeader.background": "#2D3241",
    "sideBarTitle.foreground": "#CCCCCC",
    "panel.background": "#24252E",
    "panel.border": "#6a6f81",
    "panelTitle.activeForeground": "#FFFFFF",
    "panelTitle.inactiveForeground": "#6C7390",
    "tab.activeBackground": "#2B2D3B",
    "tab.activeForeground": "#FFFFFF",
    "tab.border": "#6a6f81",
    "tab.inactiveBackground": "#24252E",
    "tab.inactiveForeground": "#6C7390",
    "titleBar.activeBackground": "#24252E",
    "titleBar.activeForeground": "#CCCCCC",
    "titleBar.inactiveBackground": "#1A1B24",
    "titleBar.inactiveForeground": "#6C7390",
    "dropdown.background": "#2D3241",
    "dropdown.foreground": "#CCCCCC",
    "input.background": "#262a37",
    "input.foreground": "#CCCCCC",
    "list.activeSelectionBackground": "#265AA9",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.hoverBackground": "#2D3241",
    "list.hoverForeground": "#FFFFFF",
    "statusBar.background": "#24252E",
    "statusBar.foreground": "#CCCCCC",
    "statusBar.border": "#6a6f81",
    "statusBar.noFolderBackground": "#24252E",
    "statusBar.debuggingBackground": "#4E77FF",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBarItem.hoverBackground": "#2D3241",
    "statusBarItem.remoteBackground": "#2D3241",
    "statusBarItem.remoteForeground": "#CCCCCC",
    "editorBracketHighlight.foreground1": "#bcbec8",
    "editorBracketHighlight.foreground2": "#5dc8bf",
    "editorBracketHighlight.foreground3": "#a073e9ff",
    "editorBracketHighlight.foreground4": "#f5ab6eff",
    "editorBracketHighlight.foreground5": "#bd80a9c2",
    "editorBracketHighlight.foreground6": "#8ac4ebff",
    "[luau]": {
      "editor.semanticHighlighting.enabled": false
    }
  },
  "semanticTokenColors": {
    "function": {
      "foreground": "#F2DAA5"
    },
    "variable": {
      "foreground": "#CCCCCC"
    },
    "property": {
      "foreground": "#61a1f1"
    },
    "variable.other.table.property.lua": {
      "foreground": "#61a1f1"
    }
  },
  "tokenColors": [
    {
      "scope": [
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#eb7973"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#F2DAA5"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#def6b5"
      }
    },
    {
      "scope": [
        "constant",
        "support.function",
        "keyword.function"
      ],
      "settings": {
        "foreground": "#8fb4ff"
      }
    },
    {
      "scope": [
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#61a1f1"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.nil"
      ],
      "settings": {
        "foreground": "#CCB8FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation.definition.parameters",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#bd80a9"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6C7390"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.bracket.level1",
      "settings": {
        "foreground": "#bcbec8",
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation.bracket.level2",
      "settings": {
        "foreground": "#5dc8bf",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.bracket.level3",
      "settings": {
        "foreground": "#a073e9ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.bracket.level4",
      "settings": {
        "foreground": "#f5ab6eff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.bracket.level5",
      "settings": {
        "foreground": "#bd80a9c2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.bracket.level6",
      "settings": {
        "foreground": "#8ac4ebff",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [
        "keyword.control.luau",
        "keyword.operator.wordlike",
        "storage.modifier.local.luau"
      ],
      "settings": {
        "foreground": "#eb7973",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.language.self.luau"
      ],
      "settings": {
        "foreground": "#eb7973",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [
        "entity.name.function.luau"
      ],
      "settings": {
        "foreground": "#F2DAA5"
      }
    },
    {
      "scope": [
        "entity.name.function-table.lua"
      ],
      "settings": {
        "foreground": "#F2DAA5"
      }
    },
    {
      "scope": [
        "variable.other.table.property.lua",
        "variable.language.metamethod.luau"
      ],
      "settings": {
        "foreground": "#61a1f1"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#def6b5"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "source.luau"
      ],
      "settings": {
        "foreground": "#bd80a9"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#eb7973"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#F2DAA5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "string.template"
      ],
      "settings": {
        "foreground": "#def6b5"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#CCB8FF"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "meta.object-literal.key",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#61a1f1"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#8fb4ff"
      }
    },
    {
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#bd80a9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": [
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#4E77FF"
      }
    },
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#6C7390",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#61a1f1"
      }
    },
    {
      "scope": [
        "string.quoted.double.json",
        "string.quoted.single.json"
      ],
      "settings": {
        "foreground": "#F2DAA5"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json"
      ],
      "settings": {
        "foreground": "#F2DAA5"
      }
    },
    {
      "scope": [
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#CCB8FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.character.escape",
        "constant.character.escape.json"
      ],
      "settings": {
        "foreground": "#eb7973",
        "fontStyle": "bold"
      }
    }
  ]
}